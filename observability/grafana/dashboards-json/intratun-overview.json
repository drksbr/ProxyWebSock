{
  "id": null,
  "uid": "intratun-overview",
  "title": "IntraTun Overview",
  "tags": ["intratun"],
  "timezone": "",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "row",
      "title": "Summary",
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0}
    },
    {
      "type": "stat",
      "title": "Agents Connected",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "intratun_agents_connected",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 1},
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "values": false},
        "orientation": "horizontal"
      }
    },
    {
      "type": "stat",
      "title": "Active Streams",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "intratun_active_streams",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 6, "w": 6, "x": 6, "y": 1},
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "values": false},
        "orientation": "horizontal"
      }
    },
    {
      "type": "timeseries",
      "title": "Traffic (Bytes/s)",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "rate(intratun_bytes_upstream_total[1m])",
          "refId": "A",
          "legendFormat": "Upstream"
        },
        {
          "expr": "rate(intratun_bytes_downstream_total[1m])",
          "refId": "B",
          "legendFormat": "Downstream"
        }
      ],
      "gridPos": {"h": 9, "w": 12, "x": 12, "y": 1}
    },
    {
      "type": "timeseries",
      "title": "Dial Errors",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "increase(intratun_dial_errors_total[5m])",
          "refId": "A",
          "legendFormat": "Dial Errors"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 7}
    },
    {
      "type": "timeseries",
      "title": "Auth Failures",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {
          "expr": "increase(intratun_auth_failures_total[5m])",
          "refId": "A",
          "legendFormat": "Auth Failures"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 10}
    }
  ],
  "templating": {
    "list": []
  }
}
